{
  "manifest_version": 2,

  "name": "fb cleanr",
  "version": "0.0.0.1",
  "description": "remove the ad-sidebar from fb",
  "author": "Armando Reed",

  "background": {
    "scripts": ["background.js"]
  },

  "icons": { "48": "icon48.png",
             "128": "icon128.png"
  },

  "offline_enabled": true,
  "permissions": [ "tabs", "https://www.facebook.com/*", "http://www.facebook.com/*"]
}